{
  "plugins": [
  ],
  "ecmaFeatures": {
  },
  "rules": {
    "block-scoped-var": 2,
    "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
    "camelcase": 0,
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "max-depth": [1, 3],
    "max-statements": [1, 15],
    "max-len": [1, 80],
    "new-cap": [1,{ "capIsNewExceptions": ["Schema", "Router"] }],
    "no-extend-native": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-vars": 1,
    "quotes": [2, "single", "avoid-escape"],
    "semi": [2, "always"],
    "space-after-keywords": [2, "always"],
    "space-in-brackets": [2, "always", {
        "singleValue": false,
        "propertyName": false
    }],
    "space-unary-ops": 2,
    "no-underscore-dangle": 0,
  }
}
